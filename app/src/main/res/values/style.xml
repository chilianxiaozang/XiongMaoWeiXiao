<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="button_model">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#DF000000</item>
        <item name="android:background">@drawable/gb_button</item>
    </style>
    <style name="textview_model">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#DF000000</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="edittext_model">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:textColorHint">#47000000</item>
    </style>
    <style name="dividers_white">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#fafafa</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
    </style>
</resources>